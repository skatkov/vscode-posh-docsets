{
	"name": "vscode-posh-docsets",
	"displayName": "vscode-posh-docsets",
	"description": "Docsets for ruby developers via POSH TUI",
	"version": "0.0.1",
	"publisher": "skatkov",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/skatkov/vscode-posh-docsets"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "poshtui.view",
				"title": "Posh: View"
			},
			{
				"command": "poshtui.docsets",
				"title": "Posh: Update Docsets"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\""
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.74.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.1.3"
	}
}
